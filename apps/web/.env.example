# Observer Web App Environment Variables

# API Server URL (required for production)
# Development: http://localhost:8000
# Production: https://your-server.railway.app
VITE_API_URL=

# WebSocket URL (required for production)
# Development: ws://localhost:8000
# Production: wss://your-server.railway.app
VITE_WS_URL=

# Railway server host for auto-detection (optional)
# Only needed if your Railway deployment has a different server service name
# Default: server-production-0b14
VITE_SERVER_RAILWAY_HOST=
